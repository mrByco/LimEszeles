{
  "name": "margarita_app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "prestart": "run-script-os",
    "prestart:default": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx ng-openapi-gen --input https://localhost:7156/swagger/v1/swagger.json --output src/app/api",
    "prestart:windows": "set NODE_TLS_REJECT_UNAUTHORIZED=0&& npx ng-openapi-gen --input https://localhost:7156/swagger/v1/swagger.json --output src/app/api",
    "start": "ng serve --configuration development",
    "build": "ng build",
    "build:dev": "ng build --configuration staging --output-hashing all",
    "build:prod": "ng build --configuration production --output-hashing all",
    "build:ssr": "ng build && ng run margarita_app:server",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "generate": "NODE_TLS_REJECT_UNAUTHORIZED=0 npx ng-openapi-gen --input https://localhost:7156/swagger/v1/swagger.json --output src/app/api",
    "lint": "ng lint",
    "dev:ssr": "ng run margarita_app:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "prerender": "ng run margarita_app:prerender",
    "ionic:build": "npm run build",
    "ionic:serve": "npm run start"
  },
  "private": true,
